    <Nullable>enable</Nullable>
    <UseWPF>true</UseWPF>
    <ApplicationIcon>assets\qrcode.ico</ApplicationIcon>
  </PropertyGroup>

  <ItemGroup>
    <None Remove="image_placeholder2.png" />
